{
  "presets": ["module:@react-native/babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".ios.js", ".android.js", ".js", ".ts", ".tsx", ".json"],
        "alias": {
          "@styles": "./src/styles",
          "@components": "./src/components",
          "@pages": "./src/pages"
        }
      }
    ]
  ]
}
